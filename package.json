{
    "name": "puppapp",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "run-android-local": "ENVFILE=.env.local react-native run-android",
        "run-android-staging": "ENVFILE=.env.staging react-native run-android",
        "run-android-production": "ENVFILE=.env.production react-native run-android",
        "run-ios-local": "react-native run-ios --scheme \"PuppApp (local)\"",
        "run-ios-staging": "react-native run-ios  --scheme \"PuppApp (staging)\"",
        "run-ios-production": "react-native run-ios  --scheme \"PuppApp (production)\"",
        "start": "react-native start",
        "test": "jest",
        "generate-types-local": "dtsgen -o schema.d.ts --url http://localhost:3000/swagger.json -n \"\"",
        "build-android-staging": "cd android && ENVFILE=.env.staging ./gradlew assembleRelease",
        "build-android-production": "cd android && ENVFILE=.env.production ./gradlew assembleRelease"
    },
    "dependencies": {
        "@react-native-community/netinfo": "^4.6.1",
        "@react-native-firebase/app": "^6.1.0",
        "@react-native-firebase/crashlytics": "^6.1.0",
        "@reduxjs/toolkit": "^1.0.4",
        "@sentry/react-native": "^1.1.0",
        "axios": "^0.19.0",
        "formik": "^2.0.6",
        "globalbit-helpers": "vadimf/globalbit-helpers#master",
        "i18next": "^19.0.1",
        "react": "16.9.0",
        "react-i18next": "11.2.5",
        "react-native": "0.61.4",
        "react-native-config": "^0.12.0",
        "react-native-easy-toast": "^1.2.0",
        "react-native-exit-app": "^1.1.0",
        "react-native-fbsdk": "^1.1.1",
        "react-native-gesture-handler": "^1.5.0",
        "react-native-keyboard-aware-scroll-view": "^0.9.1",
        "react-native-reanimated": "^1.4.0",
        "react-native-screens": "^1.0.0-alpha.23",
        "react-native-splash-screen": "^3.2.0",
        "react-native-webview": "^7.5.2",
        "react-navigation": "^4.0.10",
        "react-navigation-hooks": "^1.1.0",
        "react-navigation-stack": "^1.10.3",
        "react-navigation-tabs": "^2.6.0",
        "react-redux": "^7.1.3",
        "socket.io-client": "^2.3.0",
        "styled-components": "^4.4.1",
        "yup": "^0.27.0"
    },
    "devDependencies": {
        "@babel/core": "^7.6.2",
        "@babel/runtime": "^7.6.2",
        "@react-native-community/eslint-config": "^0.0.5",
        "@types/axios": "^0.14.0",
        "@types/i18next": "^13.0.0",
        "@types/jest": "^24.0.18",
        "@types/node": "^12.12.14",
        "@types/react-i18next": "^8.1.0",
        "@types/react-native": "^0.60.22",
        "@types/react-native-fbsdk": "^1.0.0",
        "@types/react-navigation": "^3.4.0",
        "@types/react-redux": "^7.1.5",
        "@types/react-test-renderer": "16.9.0",
        "@types/socket.io-client": "^1.4.32",
        "@types/styled-components": "^4.4.0",
        "@types/yup": "^0.26.26",
        "babel-jest": "^24.9.0",
        "jest": "^24.9.0",
        "metro-react-native-babel-preset": "^0.56.0",
        "react-test-renderer": "16.9.0",
        "typescript": "^3.6.3"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
